#SQL[@17,N217]Result[]
CREATE TABLE employees (
    id INT AUTO_INCREMENT PRIMARY KEY,
    employee_name VARCHAR(255) NOT NULL,
    employee_hire_date DATE,
    company_founded DATE,
    employee_status ENUM('active', 'inactive') NOT NULL
);

