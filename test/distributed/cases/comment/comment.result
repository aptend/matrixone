  `inspection_content` varchar(50) DEFAULT NULL COMMENT '点检内容',
  `inspection_tool` varchar(50) DEFAULT NULL COMMENT '点检工具',
  `inspection_method` varchar(50) DEFAULT NULL COMMENT '点检方法',
  `judgment_criteria` varchar(50) DEFAULT NULL COMMENT '判断标准',
  `inspection_exceptions` text DEFAULT NULL COMMENT '异常描述["表面有磨损","表面有裂痕"]',
  `major` text DEFAULT NULL COMMENT '点检专业',
  `cycle_unit` varchar(50) DEFAULT NULL COMMENT '执行周期,一次、年、季度、月、周、日、班次、小时',
  `cycle_num` decimal(20,6) DEFAULT NULL COMMENT '周期间隔',
  `remark` varchar(255) DEFAULT NULL COMMENT '备注',
  `create_org` bigint DEFAULT NULL COMMENT '创建组织',
  `use_org` bigint DEFAULT NULL COMMENT '使用组织',
  `create_time` datetime NOT NULL COMMENT '创建时间',
  `create_user` bigint DEFAULT NULL COMMENT '创建人员',
  `create_user_name` varchar(50) DEFAULT NULL,
  `create_dept` bigint DEFAULT NULL COMMENT '创建部门',
  `update_time` datetime DEFAULT NULL COMMENT '更新时间',
  `update_user` bigint DEFAULT NULL COMMENT '更新人员',
  `update_user_name` varchar(50) DEFAULT NULL,
  `status` int DEFAULT NULL,
  `is_deleted` int DEFAULT NULL COMMENT '是否已删除',
  PRIMARY KEY (`id`)
) COMMENT='设备点检标准表'

