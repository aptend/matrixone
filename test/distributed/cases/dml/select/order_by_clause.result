#SQL[@1,N25]Result[]
DROP TABLE IF EXISTS dept;

#SQL[@2,N224]Result[]
create table dept(
    deptno int unsigned auto_increment COMMENT '部门编号',
    dname varchar(15) COMMENT '部门名称',
    loc varchar(50)  COMMENT '部门所在位置',
    primary key(deptno)
) COMMENT='部门表';

